{
    "exclude": [
        "node_modules",
        "build",
        "build-bookmarklet",
        "build-browserext"
    ],
    "compilerOptions": {
        "checkJs": true,
        "noEmit": true,
        "noImplicitReturns": true,
        "alwaysStrict": true,
        "declaration": false,
        "noImplicitAny": true,
        "maxNodeModuleJsDepth": 1,
        "target": "es2015",
        "moduleResolution": "node"
    }
}